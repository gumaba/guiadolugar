          <img src="img/logo.png">
        </div>
        <div style="text-align: left">
          <%= yield %>
        </div>
        <hr>

      </div>
    </div>
  </div>
</div>
<footer>
  <p>&copy; <%= Configure::get('company_name') %> <%= Time.new.year %></p>
